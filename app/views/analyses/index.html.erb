<p id="notice"><%= notice %></p>

<h1>Analyses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Descriptioni</th>
      <th>Seq blood1</th>
      <th>Seq blood2</th>
      <th>Seq brain1</th>
      <th>Seq brain2</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analyses.each do |analysis| %>
      <tr>
        <td><%= analysis.name %></td>
        <td><%= analysis.descriptioni %></td>
        <td><%= analysis.seq_blood1 %></td>
        <td><%= analysis.seq_blood2 %></td>
        <td><%= analysis.seq_brain1 %></td>
        <td><%= analysis.seq_brain2 %></td>
        <td><%= analysis.project %></td>
        <td><%= link_to 'Show', analysis %></td>
        <td><%= link_to 'Edit', edit_analysis_path(analysis) %></td>
        <td><%= link_to 'Destroy', analysis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis', new_analysis_path %>
