
        <div class="clearfix"></div>
        <div class="row">
          <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
              <div class="x_title">
                <!-- %= @title % -->
                <%= @title %>
                <div class="clearfix"></div>
              </div>
              <div class="x_content">
                <br />
                  <%= form_with(model: [@project, @analysis], class: "form-horizontal form-label-left", id: "demo-form2") do |form| %>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Name <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.text_field :name, id: :analysis_name, class: 'form-control col-md-7 col-xs-12', required: true %>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12">Content <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.text_area :description, id: :analysis_description, cols: 80, rows: 5, class: 'resizable_textarea form-control', placeholder: 'description' %>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Blood Sequence file 1(FASTQ) <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.file_field :seq_blood1, id: :analysis_seq_blood1,  class: 'form-control col-md-7 col-xs-12', required: true %>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Blood Sequence file 2(FASTQ) <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.file_field :seq_blood2, id: :analysis_seq_blood2,  class: 'form-control col-md-7 col-xs-12', required: true %>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Brain Sequence file 1(FASTQ) <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.file_field :seq_brain1, id: :analysis_seq_brain1,  class: 'form-control col-md-7 col-xs-12', required: true %>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Brain Sequence file 2(FASTQ) <span class="required">*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                      <%= form.file_field :seq_brain2, id: :analysis_seq_brain2,  class: 'form-control col-md-7 col-xs-12', required: true %>
                    </div>
                  </div>
                  <%= form.hidden_field :project_id, id: :analysis_project_id %>
                  <div class="ln_solid"></div>
                  <div class="form-group">
                    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                      <button class="btn btn-primary" type="button">Cancel</button>
				              <button class="btn btn-primary" type="reset">Reset</button>
                      <button type="submit" class="btn btn-success">Submit</button>
                    </div>
                  </div>

                <% end %>
              </div>
            </div>
          </div>
        </div>

