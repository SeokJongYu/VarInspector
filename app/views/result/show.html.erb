<% provide(:title, "Result | #{@analysis.name}") %>


<div class="page-title">
    <div class="title_left">
    <h3><%= @analysis.name %></h3>
    </div>

    <div class="title_right">
    <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
        <div class="input-group">
        <input type="text" class="form-control" placeholder="Search for...">
        <span class="input-group-btn">
            <button class="btn btn-default" type="button">Go!</button>
        </span>
        </div>
    </div>
    </div>
</div>

<div class="clearfix"></div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
        <div class="x_title">
        <h2>Analysis Result <small><%= @analysis.name %></small></h2>
        <ul class="nav navbar-right panel_toolbox">
            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
            <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
            <ul class="dropdown-menu" role="menu">
                <li><a href="#">Settings 1</a>
                </li>
                <li><a href="#">Settings 2</a>
                </li>
            </ul>
            </li>
            <li><a class="close-link"><i class="fa fa-close"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
        </div>
        <div class="x_content">
        <p class="text-muted font-13 m-b-30">
        <%= @analysis.description %>
        </p>
        <p> 
            <%= link_to "Excel", result_show_path( :result_id => params[:result_id], :analysis_id => params[:analysis_id], :format => "xls"), :class=>"btn btn-primary" %> 
        </p> 
        <table id="datatable" class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>Tool</th>
                <th>CHROM</th>
                <th>POS</th>
                <th>RS_ID</th>
                <th>REF</th>
                <th>ALT</th>
                <th>FORMAT</th>
                <th>EB</th>
                <th>GEN_DP</th>
                <th>GEN_AU</th>
                <th>GEN_CU</th>
                <th>GEN_GU</th>
                <th>GEN_TU</th>
                <th>GEN_AD</th>
                <th>ANN_IMPACT</th>
                <th>ANN_GENE</th>
                <th>ANN_EFFECT</th>
                <th>ANN_HGVS_C</th>
                <th>ANN_HGVS_P</th>
                <th>dbNSFP_SIFT_pred</th>
                <th>dbNSFP_Polyphen2_HDIV_pred</th>
                <th>dbNSFP_Polyphen2_HVAR_pred</th>
                <th>dbNSFP_MutationTaster_pred</th>
                <th>dbNSFP_GERP_NR</th>
                <th>dbNSFP_GERP_RS</th>
                <th>dbNSFP_phastCons100way_vertebrate</th>
                <th>dbNSFP_CADD_phred</th>
                <th>dbNSFP_ExAC_AC</th>
                <th>dbNSFP_ExAC_AF</th>
                <th>dbNSFP_ExAC_Adj_AC</th>
                <th>dbNSFP_ExAC_Adj_AF</th>
                <th>dbNSFP_ExAC_EAS_AC</th>
                <th>dbNSFP_ExAC_EAS_AF</th>
                <th>dbNSFP_ExAC_SAS_AC</th>
                <th>dbNSFP_ExAC_SAS_AF</th>
                <th>dbNSFP_ExAC_AFR_AC</th>
                <th>dbNSFP_ExAC_AFR_AF</th>
                <th>dbNSFP_ExAC_AMR_AC</th>
                <th>dbNSFP_ExAC_AMR_AF</th>
                <th>dbNSFP_ExAC_NFE_AC</th>
                <th>dbNSFP_ExAC_NFE_AF</th>
                <th>dbNSFP_ExAC_FIN_AC</th>
                <th>dbNSFP_ExAC_FIN_AF</th>

            </tr>
            </thead>

            <tbody>

            <% @results.each do |result| %>
                <tr>
                    <td><%= result.tool %></td>
                    <td><%= result.CHROM %></td>
                    <td><%= result.POS %></td>
                    <td><%= result.RS_ID %></td>
                    <td><%= result.REF %></td>
                    <td><%= result.ALT %></td>
                    <td><%= result.FORMAT %></td>
                    <td><%= result.EB %></td>
                    <td><%= result.GEN_DP %></td>
                    <td><%= result.GEN_AU %></td>
                    <td><%= result.GEN_CU %></td>
                    <td><%= result.GEN_GU %></td>
                    <td><%= result.GEN_TU %></td>
                    <td><%= result.GEN_DP %></td>
                    <td><%= result.GEN_AD %></td>
                    <td><%= result.ANN_IMPACT %></td>
                    <td><%= result.ANN_GENE %></td>
                    <td><%= result.ANN_EFFECT %></td>
                    <td><%= result.ANN_HGVS_C %></td>
                    <td><%= result.ANN_HGVS_P %></td>
                    <td><%= result.dbNSFP_SIFT_pred %></td>
                    <td><%= result.dbNSFP_Polyphen2_HDIV_pred %></td>
                    <td><%= result.dbNSFP_Polyphen2_HVAR_pred %></td>
                    <td><%= result.dbNSFP_MutationTaster_pred %></td>
                    <td><%= result.dbNSFP_GERP_NR %></td>
                    <td><%= result.dbNSFP_GERP_RS %></td>
                    <td><%= result.dbNSFP_phastCons100way_vertebrate %></td>
                    <td><%= result.dbNSFP_CADD_phred %></td>
                    <td><%= result.dbNSFP_ExAC_AC %></td>
                    <td><%= result.dbNSFP_ExAC_AF %></td>
                    <td><%= result.dbNSFP_ExAC_Adj_AC %></td>
                    <td><%= result.dbNSFP_ExAC_Adj_AF %></td>
                    <td><%= result.dbNSFP_ExAC_EAS_AC %></td>
                    <td><%= result.dbNSFP_ExAC_EAS_AF %></td>
                    <td><%= result.dbNSFP_ExAC_SAS_AC %></td>
                    <td><%= result.dbNSFP_ExAC_SAS_AF %></td>
                    <td><%= result.dbNSFP_ExAC_AFR_AC %></td>
                    <td><%= result.dbNSFP_ExAC_AFR_AF %></td>      
                    <td><%= result.dbNSFP_ExAC_AMR_AC %></td>
                    <td><%= result.dbNSFP_ExAC_AMR_AF %></td>
                    <td><%= result.dbNSFP_ExAC_NFE_AC %></td>
                    <td><%= result.dbNSFP_ExAC_NFE_AF %></td>
                    <td><%= result.dbNSFP_ExAC_FIN_AC %></td>
                    <td><%= result.dbNSFP_ExAC_FIN_AF %></td>
                            
                </tr>
            <% end %>

            </tbody>
        </table>
    </div>
</div>

